{
  "rewrites": [
    {
      "source": "/",
      "destination": "/code/index.html"
    },
    {
      "source": "/:page",
      "destination": "/code/:page.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
